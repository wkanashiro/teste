<?xml version="1.0" encoding="UTF-8"?>
<action-mapping>
	<action
		name="list"
		label="List Authors | pt_BR: Listar Cursos"
		default="true"
		description="">
		<menu action="create" />
		<menu function="search" />
	</action>
	<action
		name="view"
		label="View Author | pt_BR: Visualizar Curso"
		description="">
		<menu action="list" />
