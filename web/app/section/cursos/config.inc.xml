	</action>
	<action
		name="create"
		label="Create New Author | pt_BR: Criar Novo Curso"
		description="">
		<menu function="save" />
		<menu action="list" image="close.png" />
	</action>
	<action
		name="edit"
		label="Edit Author | pt_BR: Editar Curso"
		description="">
		<menu function="save" />
		<menu action="list" image="close.png" />
	</action>
	<action
		name="delete"
		label="Delete Author | pt_BR: Apagar Curso"
		description="">
		<menu function="delete" />
		<menu action="list" image="close.png" />
	</action>
</action-mapping>